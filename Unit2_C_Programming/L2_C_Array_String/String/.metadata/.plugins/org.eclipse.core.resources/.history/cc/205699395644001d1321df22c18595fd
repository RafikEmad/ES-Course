#include "stdio.h"
#include "string.h"

int main()
{
	char a[100];
	int length=0;


	fflush(stdin);fflush(stdout);
	printf("Enter a string: ");

	fflush(stdin);fflush(stdout);
	gets(a);

	int i=0;

	while (a[i]!=0)
	{
		length++;
		i++;
	}
	fflush(stdin);fflush(stdout);
	printf("Length of string: %d",length);

	return 0;
}
